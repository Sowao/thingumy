<div class="row artwork-header">
  <div class="col-xs-2"></div>
  <div class="col-xs-7">
    <div class="row">
      <div class="col-xs-12 artist-name">
        <h2 class="exfrasis-heading">
          <%= @artwork.artist_name %>
        </h2>
      </div>
      <div class="col-xs-12 artwork-title">
        <%= @artwork.title %>
      </div>
    </div>
  </div>
  <div class="col-xs-3">
    HEART
  </div>
</div>

<% @artwork.images.each do |image| %>
  <div class="col-xs-12 artwork-image">
    <%= image_tag image.url %>
  </div>
<% end %>

